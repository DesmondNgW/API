<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <AppSettings>
    <!--log4net日志根目录-->
    <add key="Log4NetBaseDirectory" value="" />
    <!--debug 客户号-->
    <add key="CustomerNo" value="" />
    <!--Key缓存版本号，修改使所有缓存失效-->
    <add key="CacheKeyVersion" value="0.0.1" />
    <!--CookieDomain-->
    <add key="CookieDomain" value="" />
    <!--RedisCacheEnable-->
    <add key="RedisCacheEnable" value="false" />
    <!--MongoDbEnable-->
    <add key="MongoDbEnable" value="true" />
    <!--Debug-->
    <add key="Debug" value="true" />
  </AppSettings>
</configuration>